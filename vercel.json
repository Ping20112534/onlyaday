{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/admin",
        "dest": "admin.html"
      },
      {
        "src": "/display",
        "dest": "display.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  